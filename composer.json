{
    "name": "phink/phink",
    "type": "library",
    "description": "Think ${PHP} Different",
    "homepage": "https://phink.org",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "David Blanchard",
            "email": "dblanchard1@bbox.fr",
            "homepage": "https://code-phoenix.org",
            "role": "Developer"
        }
    ],
    "autoload": {
        "classmap": [
            "src/phink/"
        ],
        "exclude-from-classmap": [
            "src/phink/phink_library.php"
        ],
        "psr-4": {
            "Phink" : ""
        },
        "files": [
            "src/phink/core/constants.php"
        ]
    },
    "require": {
        "php": ">=7.0.0",
        "twig/twig": "^2.0",
        "phink/phinkjs": "v0.2.*",
        "components/jquery": ">=1.9.1",
        "components/jqueryui": ">=1.12.1",
        "components/font-awesome": "=4.7.0",
        "components/bootstrap": "=3.3.5",
        "components/bootstrap-default": "=3.3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "php-coveralls/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "include-path": [
        "src"
    ]
}